<div class="container mt-5">
	<h1 class=" text-center">Vista Productos</h1>
	<table class="table table-striped table-hover mt-4">
		<thead>
			<tr class="table-primary border-2 border-primary">
				<th class="border__list text-center" scope="col">Nombre</th>
				<th class="border__list text-center" scope="col">Precio</th>
				<th class="border__list text-center" scope="col">foto</th>
			</tr>
		</thead>
		<tbody class="table-primary border-2 border-primary" id="tabProd">
			<template id="template-inventario">
				<tr class="text-center">
					<th class="border__list text-center" scope="row">{{title}}</th>
					<td class="border__list text-center" scope="col">{{price}}</td>
					<td class="border__list text-center" scope="col">{{thumbnail}}</td>
				</tr>
			</template>
		</tbody>
	</table>
</div>